                  <RadioButton fx:id="wRestartYesBtn" disable="true" mnemonicParsing="false" onAction="#restartGame" text="Yes" visible="false">
                     <toggleGroup>
                        <ToggleGroup fx:id="wRestart" />
                     </toggleGroup></RadioButton>
                  <RadioButton fx:id="wRestartNoBtn" disable="true" mnemonicParsing="false" text="No" toggleGroup="$wRestart" visible="false" />
               </children>
            </VBox>
         </children>
      </VBox>
   </left>
   <right>
      <VBox prefHeight="200.0" prefWidth="150.0" spacing="10.0" BorderPane.alignment="CENTER">
         <children>
            <Label prefHeight="172.0" prefWidth="151.0" text="Please input two players' names and choose the game template before you start the game." wrapText="true">
               <padding>
                  <Insets left="15.0" right="15.0" />
               </padding></Label>
            <Label alignment="CENTER" prefHeight="20.0" prefWidth="153.0" text="Black's name:" />
            <TextField fx:id="bNameField" />
            <Label alignment="CENTER" prefHeight="20.0" prefWidth="153.0" text="White's name:" />
            <TextField fx:id="wNameField" />
            <Label alignment="CENTER" prefHeight="20.0" prefWidth="151.0" text="Game modes:" />
            <RadioButton fx:id="classicModeBtn" mnemonicParsing="false" prefHeight="20.0" prefWidth="152.0" selected="true" text="classic game">
               <toggleGroup>
                  <ToggleGroup fx:id="gameMode" />
               </toggleGroup>
            </RadioButton>
            <RadioButton fx:id="newModeBtn" mnemonicParsing="false" text="new game" toggleGroup="$gameMode" />
            <Pane prefHeight="45.0" prefWidth="200.0">
               <children>
                  <Button fx:id="startBtn" layoutX="40.0" layoutY="8.0" mnemonicParsing="false" onAction="#firstStartGame" styleClass="func_btn" text="start" />
               </children>
            </Pane>
         </children>
      </VBox>
   </right>
   <bottom>
      <FlowPane fx:id="resultFlowPane" hgap="10.0" orientation="VERTICAL" prefHeight="200.0" prefWidth="200.0" vgap="10.0" BorderPane.alignment="CENTER">
         <children>
            <Label text="Result:" />
         </children>
         <padding>
            <Insets bottom="5.0" left="10.0" right="10.0" top="5.0" />
         </padding>
      </FlowPane>
   </bottom>
</BorderPane>
